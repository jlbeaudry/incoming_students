  volume = {86},
  pages = {948--955},
  issn = {0006-3444, 1464-3510},
  doi = {10.1093/biomet/86.4.948},
  abstract = {An appealing feature of multiple imputation is the simplicity of the rules for combining the multiple complete-data inferences into a final inference, the repeated-imputation inference (Rubin, 1987). This inference is based on a t distribution and is derived from a Bayesian paradigm under the assumption that the complete-data degrees of freedom, {$\nu$}com, are infinite, but the number of imputations, m, is finite. When {$\nu$}com is small and there is only a modest proportion of missing data, the calculated repeated-imputation degrees of freedom, {$\nu$}m, for the t reference distribution can be much larger than {$\nu$}com, which is clearly inappropriate. Following the Bayesian paradigm, we derive an adjusted degrees of freedom, {$\nu\tilde$}m, with the following three properties: for fixed m and estimated fraction of missing information, {$\nu\tilde$}m monotonically increases in {$\nu$}com; {$\nu\tilde$}m is always less than or equal to {$\nu$}com; and {$\nu\tilde$}m equals {$\nu$}m when {$\nu$}com is infinite. A small simulation study demonstrates the superior frequentist performance when using {$\nu\tilde$}m rather than {$\nu$}m.},
  file = {C\:\\Users\\mnwillia\\Zotero\\storage\\7SNZI7TH\\Barnard and Rubin - 1999 - Miscellanea. Small-sample degrees of freedom with .pdf},
  journal = {Biometrika},
  keywords = {Bayesian inference; fraction of missing information; missing at random; missing data mechanism; repeated imputation},
  language = {en},
  number = {4}
}

