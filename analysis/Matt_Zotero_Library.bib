@article{spreux-varoquauxDifferentialAbnormalitiesPlasma2001,
  title = {Differential Abnormalities in Plasma 5-{{HIAA}} and Platelet Serotonin Concentrations in Violent Suicide Attempters {{Relationships}} with Impulsivity and Depression},
