  copyright = {\textcopyright{} 2020 British Association of Critical Care Nurses},
  file = {C\:\\Users\\mnwillia\\Zotero\\storage\\E6PMX56X\\Leng et al. - Mental distress and influencing factors in nurses .pdf},
  journal = {Nursing in Critical Care},
  keywords = {COVID-19,critical care,mental distress,nurses,PSS-14,PTSD symptoms},
  language = {en},
  number = {n/a}
}

