@article{andreVaccinationGreatlyReduces2008,
  title = {Vaccination Greatly Reduces Disease, Disability, Death and Inequity Worldwide},
  author = {Andre, F. E. and Booy, R. and Bock, H. L. and Clemens, J. and Datta, S. K. and John, T. J. and Lee, B. W. and Lolekha, S. and Peltola, H. and Ruff, T. A. and Santosham, M. and Schmitt, H. J.},
  year = {2008},
  month = feb,
  volume = {86},
  pages = {140--146},
  issn = {0042-9686, 0042-9686},
  doi = {10.1590/S0042-96862008000200016},
  abstract = {In low-income countries, infectious diseases still account for a large proportion of deaths, highlighting health inequities largely caused by economic differences. Vaccination can cut health-care costs and reduce these inequities. Disease control, elimination or eradication can save billions of US dollars for communities and countries. Vaccines have lowered the incidence of hepatocellular carcinoma and will control cervical cancer. Travellers can be protected against "exotic" diseases by appropriate vaccination. Vaccines are considered indispensable against bioterrorism. They can combat resistance to antibiotics in some pathogens. Noncommunicable diseases, such as ischaemic heart disease, could also be reduced by influenza vaccination. Immunization programmes have improved the primary care infrastructure in developing countries, lowered mortality in childhood and empowered women to better plan their families, with consequent health, social and economic benefits. Vaccination helps economic growth everywhere, because of lower morbidity and mortality. The annual return on investment in vaccination has been calculated to be between 12\% and 18\%. Vaccination leads to increased life expectancy. Long healthy lives are now recognized as a prerequisite for wealth, and wealth promotes health. Vaccines are thus efficient tools to reduce disparities in wealth and inequities in health.},
  file = {C\:\\Users\\mnwillia\\Zotero\\storage\\MSDFE5H8\\Andre et al. - 2008 - Vaccination greatly reduces disease, disability, d.pdf},
  journal = {Bulletin of the World Health Organization},
  language = {en}
}

@article{andrewsConstructValidityError1984,
  title = {Construct Validity and Error Components of Survey Measures: {{A}} Structural Modeling Approach},
  shorttitle = {Construct {{Validity}} and {{Error Components}} of {{Survey Measures}}},
  author = {Andrews, Frank M.},
  year = {1984},
  month = jun,
  volume = {48},
  pages = {409--442},
  issn = {0033-362X, 1537-5331},
  doi = {10.1086/268840},
  abstract = {Measurement errors can have profound effects on statistical relationships, and better information on the quality of measures seems needed. This study uses a new technology\textemdash structural modeling of data from special supplements to regular surveys\textemdash to generate estimates of construct validity, method effects (a major source of correlated error), and residual error (mainly random error) for a broad set of measures obtained from five national surveys and an organizational survey (total respondents = 7,706). Analysis of these estimates suggested that a typical survey item, when administered by a respected survey organization to a general population sample, can be expected to yield 50\textendash 83 percent valid variance, 0\textendash 7 percent method effects variance, and 14\textendash 48 percent residual variance. Multivariate analysis showed that over two-thirds of the variation in measurement quality could be explained by 13 survey design characteristics; characteristics of respondents explained a small additional portion. Results provide: (a) information on design conditions associated with better (or worse) measurement quality, (b) empirically based suggestions for improving measurement quality in future surveys, and (c) a set of coefficients for predicting the quality of measures not studied here.},
  file = {C\:\\Users\\mnwillia\\Zotero\\storage\\4839X47C\\Andrews - 1984 - Construct validity and error components of survey .pdf},
  journal = {Public Opinion Quarterly},
  language = {en},
  number = {2}
}

