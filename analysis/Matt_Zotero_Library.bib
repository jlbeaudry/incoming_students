  file = {C\:\\Users\\mnwillia\\Zotero\\storage\\TPD54B5B\\Lewandowsky et al. - 2015 - Recurrent Fury Conspiratorial Discourse in the Bl.pdf;C\:\\Users\\mnwillia\\Zotero\\storage\\XC47C368\\443_1.html},
