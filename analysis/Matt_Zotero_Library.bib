@misc{ConflictProjectTeams2015,
  title = {Conflict in {{Project Teams}} - {{Oxford Scholarship}}},
  year = {2015},
  month = may,
  abstract = {Project teams frequently have to deal with conflict among their team members, for instance, because project teams tend to consist of team members who differ in terms of knowledge, expertise, and experience, and/or because during a project's life cycle, the team members need to iteratively plan and define the task at hand. This chapter discusses the consequences of three types of intragroup conflict (task, relationship, and process conflict) for project team performance. It starts with a brief review of the intragroup conflict literature. Next, the chapter derives specific propositions about possible moderators of the conflict-performance relationship in project teams. These propositions highlight the crucial role of conflict management styles in shaping the impact of intragroup conflict on project team performance, as well as that of the salience of the project's finiteness, and the possibility that group members will have to work together in the near future.},
  howpublished = {http://www.oxfordscholarship.com/view/10.1093/acprof:oso/9780199861378.001.0001/acprof-9780199861378-chapter-9}
}

