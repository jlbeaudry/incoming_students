@article{borsboomWhyPsychometricsNot2004,
  title = {Why Psychometrics Is Not Pathological: {{A}} Comment on {{Michell}}},
  author = {Borsboom, Denny and Mellenbergh, Gideon J.},
  year = {2004},
  month = feb,
  volume = {14},
