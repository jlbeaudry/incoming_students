@article{gaoModelsPredictingTurnover2014,
  title = {Models for Predicting Turnover of Residential Aged Care Nurses: {{A}} Structural Equation Modelling Analysis of Secondary Data},
  shorttitle = {Models for Predicting Turnover of Residential Aged Care Nurses},
  author = {Gao, Fengsong and Newcombe, Peter and Tilse, Cheryl and Wilson, Jill and Tuckett, Anthony},
  year = {2014},
