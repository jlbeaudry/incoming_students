@article{davisOpenAccessPublishing2008,
  title = {Open Access Publishing, Article Downloads, and Citations: Randomised Controlled Trial},
  shorttitle = {Open Access Publishing, Article Downloads, and Citations},
  author = {Davis, Philip M. and Lewenstein, Bruce V. and Simon, Daniel H. and Booth, James G. and Connolly, Mathew J. L.},
