@article{fredriksonGenderAgeDifferences1996,
  title = {Gender and Age Differences in the Prevalence of Specific Fears and Phobias},
  author = {Fredrikson, Mats and Annas, Peter and Fischer, H{\AA}kan and Wik, Gustav},
  year = {1996},
